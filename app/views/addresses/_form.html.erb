<% semantic_form_for @address do |f| %>
  <% f.inputs do %>
    <%= f.input :event_type %>
      <%= f.input :street1 %>
      <%= f.input :street2 %>
      <%= f.input :city %>
      <%= f.input :state %>
      <%= f.input :country %>
      <%= f.input :zip %>
      <%= f.input :lat %>
      <%= f.input :long %>
      <%= f.input :addressible_id %>
      <%= f.input :addressible_type %>
  
  <% end %>
  <% f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
